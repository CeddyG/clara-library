{
    "name": "ceddyg/clara-library",
    "description": "Library file manager for Clara.",
    "keywords": ["laravel", "clara", "library", "file"],
    "authors": [
        {
            "name": "CeddyG"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "CeddyG\\ClaraLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CeddyG\\ClaraLibrary\\LibraryServiceProvider"
            ],
            "aliases": {
                "Library": "CeddyG\\ClaraLibrary\\Facades\\Library"
            }
        }
    }
}
